# {{ ansible_managed }}
*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [37:13960]
-A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
-A INPUT -p icmp -j ACCEPT
-A INPUT -i lo -j ACCEPT
-A INPUT -p tcp -m state --state NEW -m tcp --dport 22 -m comment --comment "Allow SSH Access"  -j ACCEPT 
-A INPUT -p tcp -m state --state NEW -m tcp --dport 80 -m comment --comment "Allow Port Web"  -j ACCEPT
-A INPUT -s 192.168.121.101 -p tcp -m tcp --dport 8080 -m comment --comment "Allow Access to APP Server VIA Front"  -j ACCEPT
-A INPUT -s 192.168.121.102 -p tcp -m tcp --dport 11211 -m comment --comment "Allow Access to Memcache Via APP To FRONT" -j ACCEPT
-A INPUT -j REJECT --reject-with icmp-host-prohibited
-A FORWARD -j REJECT --reject-with icmp-host-prohibited
COMMIT
